{
  "name": "@mlaursen/eslint-config",
  "type": "module",
  "version": "6.1.0",
  "description": "An eslint config used by mlaursen for most projects.",
  "repository": "https://github.com/mlaursen/eslint-config.git",
  "author": "Mikkel Laursen <mlaursen03@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prepare": "husky",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "watch-typecheck": "pnpm run typecheck --watch",
    "format": "prettier --write .",
    "clean": "rm -rf dist",
    "build": "tsc -p tsconfig.build.json",
    "run-script": "tsx --tsconfig scripts/tsconfig.json",
    "release": "pnpm run run-script scripts/release.ts"
  },
  "files": [
    "dist/"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "bugs": {
    "url": "https://github.com/mlaursen/eslint-config/issues"
  },
  "keywords": [
    "eslint",
    "typescript"
  ],
  "dependencies": {
    "@eslint/compat": "^1.2.7",
    "@eslint/js": "^9.22.0",
    "@next/eslint-plugin-next": "^15.2.3",
    "@typescript-eslint/utils": "^8.27.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-testing-library": "^7.1.1",
    "typescript-eslint": "^8.27.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.28.1",
    "@inquirer/prompts": "^7.4.0",
    "@octokit/core": "^6.1.4",
    "@types/node": "^22.13.10",
    "dotenv": "^16.4.7",
    "eslint": "^9.22.0",
    "husky": "^9.1.7",
    "inquirer": "^12.5.0",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "eslint": ">= 9.0.0",
    "typescript": ">= 5"
  },
  "peerDependenciesMeta": {
    "eslint": {
      "optional": false
    },
    "typescript": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,md,yml,yaml,json}": [
      "prettier --write"
    ]
  },
  "volta": {
    "node": "22.14.0",
    "pnpm": "10.6.5"
  }
}
