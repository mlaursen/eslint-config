{
  "name": "@mlaursen/eslint-config",
  "type": "module",
  "version": "8.0.2",
  "description": "An eslint config used by mlaursen for most projects.",
  "repository": "https://github.com/mlaursen/eslint-config.git",
  "author": "Mikkel Laursen <mlaursen03@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prepare": "husky",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "watch-typecheck": "pnpm run typecheck --watch",
    "format": "prettier --write .",
    "clean": "rm -rf dist",
    "build": "tsc -p tsconfig.build.json",
    "release": "tsx scripts/release.ts"
  },
  "files": [
    "dist/"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "bugs": {
    "url": "https://github.com/mlaursen/eslint-config/issues"
  },
  "keywords": [
    "eslint",
    "typescript"
  ],
  "dependencies": {
    "@eslint/compat": "^1.3.1",
    "@eslint/js": "^9.31.0",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@typescript-eslint/utils": "^8.38.0",
    "@vitest/eslint-plugin": "^1.3.4",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-testing-library": "^7.3.0",
    "typescript-eslint": "^8.38.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@mlaursen/release-script": "^0.0.3",
    "@types/node": "^22.15.29",
    "eslint": "^9.31.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "eslint": ">= 9.0.0",
    "typescript": ">= 5"
  },
  "peerDependenciesMeta": {
    "eslint": {
      "optional": false
    },
    "typescript": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,md,yml,yaml,json}": [
      "prettier --write"
    ]
  },
  "volta": {
    "node": "22.14.0",
    "pnpm": "10.6.5"
  }
}
